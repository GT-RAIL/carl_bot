<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="carl">

  <xacro:include filename="$(find jaco_model)/urdf/jaco_arm.xacro" />
  
  <link name="base_link">
   <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>    
      <geometry>
        <mesh filename="package://carl_description/meshes/Base.dae" />
      </geometry>
    </visual>
  </link>
  
  <link name="LeftWheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://carl_description/meshes/Wheel.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 .025" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.25" length="0.1" />
      </geometry>
    </collision>
  </link>
  
  <link name="RightWheel_link">
    <visual>    
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://carl_description/meshes/Wheel.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 .025" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.25" length="0.1" />
      </geometry>
    </collision>
  </link>

  <link name="CasterPlate_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5708"/>
      <geometry>
        <mesh filename="package://carl_description/meshes/casterPlate.dae" />
      </geometry>
    </visual>
  </link>

  <link name="Caster_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5708"/>
      <geometry>
        <mesh filename="package://carl_description/meshes/caster.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.09" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.116" length="0.213" />
       </geometry>
    </collision>
  </link>
  
  <link name="RearLeftStrut_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 -1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/RearStrut.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 -0.4 0.5275" rpy="0 0 0" />
      <geometry>
        <box size="0.027 0.054 1.055" />
       </geometry>
    </collision>
  </link>
  
  <link name="RearRightStrut_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 -1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/RearStrut.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0.4 0.5275" rpy="0 0 0" />
      <geometry>
        <box size="0.027 0.054 1.055" />
      </geometry>
    </collision>
  </link>

  <link name="TopPlate_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 -1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/TablePlate.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="-.29 0 -0.299" rpy="0 0 0" />
      <geometry>
        <box size="0.495 0.59 0.65" />
      </geometry>
    </collision>
  </link>
  
  <link name="CameraLift_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/CameraLift.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/CameraLift.dae" />
      </geometry>
    </collision>
  </link>
  
  <link name="CameraTiltBracket_link">
	<!--exists to move center of rotation for camera mount-->
  </link>
  
  <link name="CameraTiltPlate_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 1.5708 0" />
      <geometry>
        <mesh filename="package://carl_description/meshes/CameraPlate.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5708 0" />
      <geometry>
        <mesh filename="package://carl_description/meshes/CameraPlate.dae" />
      </geometry>
    </collision>
  </link>
  
  <link name="JacoMount_link">
    <visual>      
      <origin xyz="0 0 0" rpy="0 0 1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/JacoMount.dae" />
      </geometry>
    </visual>
  </link>

  <link name="Hokuyo_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://carl_description/meshes/Hokuyo.dae" />
      </geometry>
    </visual>
  </link>
 
   <link name="LeftSideWall_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/SideWall.dae" />
      </geometry>
    </visual>
  </link>

   <link name="RightSideWall_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/SideWall.dae" />
      </geometry>
    </visual>
  </link>
  
  <link name="FrontCover_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5708" />
      <geometry>
        <mesh filename="package://carl_description/meshes/FrontCover.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="-.245 0 -.241" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://carl_description/meshes/FrontCoverCollision.STL" />
      </geometry>
    </collision>
  </link>
  
  <link name="BackCover_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
      	<!--<box size="0.005 0.45 0.6" />-->
      	<mesh filename="package://carl_description/meshes/BackCover.dae" />
      </geometry>
    </visual>
  </link>
  
  <joint name="BaseToLeftWheel" type="continuous">
    <parent link="base_link"/>
    <child link="LeftWheel_link"/>
    <origin xyz="0 0.25 0" rpy="-1.5708 0 0" />
    <axis xyz="0 0 1" />
  </joint>
  
  <joint name="BaseToRightWheel" type="continuous">
    <parent link="base_link"/>
    <child link="RightWheel_link"/>
    <origin xyz="0 -0.25 0" rpy="1.5708 0 0" />
    <axis xyz="0 0 1" />
  </joint>
  
  <joint name="BaseToCasterPlate" type="fixed">
    <parent link="base_link"/>
    <child link="CasterPlate_link"/>
    <origin xyz="0.2883 0 0" rpy="0 0 0" />
  </joint>

  <joint name="CasterPlateToCaster" type="continuous">
    <parent link="CasterPlate_link"/>
    <child link="Caster_link"/>
    <origin xyz="0.0267 0 -0.024" rpy="0 0 0" />
    <axis xyz="0 0 1" />
  </joint>

  <joint name="BaseToRearLeftStrut" type="fixed">
    <parent link="base_link"/>
    <child link="RearLeftStrut_link"/>
    <origin xyz="-0.222 0.200 -0.102" rpy="0 0 0" />
  </joint>

  <joint name="BaseToRearRightStrut" type="fixed">
    <parent link="base_link"/>
    <child link="RearRightStrut_link"/>
    <origin xyz="-0.222 -0.200 -0.102" rpy="0 0 0" />
  </joint>

  <joint name="RearLeftStrutToTopPlate" type="fixed">
    <parent link="RearRightStrut_link"/>
    <child link="TopPlate_link"/>
    <origin xyz="0.516 0.20 0.598" rpy="0 0 0" />
  </joint>
  
  <joint name="RearLeftStrutToCameraLift" type="fixed">
    <parent link="RearRightStrut_link"/>
    <child link="CameraLift_link"/>
    <origin xyz="0.01 0.20 1.461" rpy="0 0 0" />
  </joint>
  
    <joint name="CameraLiftToCameraTiltBracket" type="continuous">
    <parent link="CameraLift_link"/>
    <child link="CameraTiltBracket_link"/>
    <origin xyz="0.03 -0.10 0" rpy="1.5708 0 0" />
    <axis xyz="0 0 1" />
  </joint>
  
  <joint name="CameraLiftBracketToCameraTiltPlate" type="fixed">
    <parent link="CameraTiltBracket_link"/>
    <child link="CameraTiltPlate_link"/>
    <origin xyz="0.11 0 -0.10" rpy="0 0 0" />
  </joint>

  <joint name="TopPlateToJacoMount" type="fixed">
    <parent link="TopPlate_link"/>
    <child link="JacoMount_link"/>
    <origin xyz="0 0 -0.13" rpy="0 0 0" />
  </joint>

  <joint name="CasterToHokuyo" type="fixed">
    <parent link="CasterPlate_link"/>
    <child link="Hokuyo_link"/>
    <origin xyz="0.07 0 0.035" rpy="0 0 0" />
  </joint>
  
  <joint name="RearLeftStrutToLeftSideWall" type="fixed">
    <parent link="RearLeftStrut_link"/>
    <child link="LeftSideWall_link"/>
    <origin xyz="0.229 0.026 0.447" rpy="0 0 0" />
  </joint>
  
  <joint name="RearRightStrutToRightSideWall" type="fixed">
    <parent link="RearRightStrut_link"/>
    <child link="RightSideWall_link"/>
    <origin xyz="0.229 -0.026 0.447" rpy="0 0 0" />
  </joint>

  <joint name="BaseToFrontCover" type="fixed">
    <parent link="base_link"/>
    <child link="FrontCover_link"/>
    <origin xyz=".247 0 0.242" rpy="0 0 0" />
  </joint>
  
  <joint name="BaseToBackCover" type="fixed">
    <parent link="base_link"/>
    <child link="BackCover_link"/>
    <origin xyz="-.237 0 -0.102" rpy="0 0 0" />
  </joint>

  <xacro:jaco_arm  parent="JacoMount_link"/>

</robot>
