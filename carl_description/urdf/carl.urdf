<robot name="carl">

  <link name="base_link">
   <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://carl_description/meshes/FullRobot - RMP_210-1 RMP_Powerbase-1.STL" />
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <geometry>
        <mesh filename="package://carl_description/meshes/FullRobot - RMP_210-1 RMP_Powerbase-1.STL"/>
      </geometry>
    </collision>
  </link>
  
  
  <link name="LeftWheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://carl_description/meshes/FullRobot - RMP_210-2 StandardWheel-2.STL" />
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://carl_description/meshes/FullRobot - RMP_210-2 StandardWheel-2.STL"/>
      </geometry>
    </collision>
  </link>
  

  <!--Super Structure-->
  <joint name="BaseToLeftWheel" type="continuous">
    <parent link="base_link"/>
    <child link="LeftWheel_link"/>
    <origin xyz="0.32 0.02 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

</robot>
