<launch>
  <node name="dynamixel_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true">
    <rosparam>
      namespace: dxl_manager
      serial_ports:
        back_port:
          port_name: "/dev/ttyUSB0"
          baud_rate: 1000000
          min_motor_id: 1
          max_motor_id: 25
          update_rate: 20
    </rosparam>
  </node>

  <rosparam file="$(find carl_dynamixel)/config/carl.yaml" command="load" />
  <node name="tilt_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py" args="--manager=dxl_manager --port back_port asus_controller" />

  <rosparam file="$(find carl_dynamixel)/config/back_servos.yaml" command="load" />
  <node name="back_joints_node" pkg="carl_dynamixel" type="back_joints_node" output="screen" />
</launch>
