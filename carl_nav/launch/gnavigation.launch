<launch>
	<include file="$(find carl_nav)/examples/gmapping.launch" /> 

	<node name="move_base" pkg="move_base" type="move_base" output="screen">
    	<rosparam file="$(find carl_nav)/cfg/costmap_common_params.yaml" command="load" ns="global_costmap" />
    	<rosparam file="$(find carl_nav)/cfg/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find carl_nav)/cfg/local_costmap_params.yaml" command="load" />
     	<rosparam file="$(find carl_nav)/cfg/global_costmap_params.yaml" command="load" />
      	<rosparam file="$(find carl_nav)/cfg/base_local_planner_params.yaml" command="load" />
	</node>
</launch>
